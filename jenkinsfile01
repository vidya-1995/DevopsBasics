pipeline {
	agent { label 'maven2' }
		stages {
                stage('git_checkout') {
		   steps {
                   checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'git_cred', url: 'https://github.com/vidya-1995/DevopsBasics.git']]])
			}
		}
      stage('Build') {
		   steps {
             sh "mvn clean install"
			}
		}
  }
}
